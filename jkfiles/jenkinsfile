node {
    stage('Start Service') {
        parallel 'test': {
            echo 'test'
        }, 'deply': {
            echo 'deply'
        }
    }

    parallel 'one' : {
        stage('one') {
            echo 'one'
        }
    }, 'two' : {
        stage('two') {
            echo 'two'
        }
    }
}

parallel 'one': {
    node{
        stage('one') {
            echo 'one'
        }
    }
}, 'two': {
    node {
        stage('two') {
            echo 'two'
        }
    }
}